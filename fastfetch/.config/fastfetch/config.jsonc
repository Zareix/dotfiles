// ~/.config/fastfetch/config.jsonc
{
  "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
  "display": {
    "separator": " ï…¸ ",
    "color": {
      "title": "yellow",
      "keys": "blue"
    }
  },
  "modules": [
    "title",
    "separator",
    "os",
    "host",
    "cpu",
    "memory",
    {
      "type": "disk",
      "showReadOnly": false
    },
    "uptime",
    "packages",
    {
      "type": "localip",
      "showPrefixLen": false
    },
    {
      "key": "Tailscale IP",
      "type": "command",
      "shell": "/bin/zsh",
      "text": "/Applications/Tailscale.app/Contents/MacOS/Tailscale ip -4 || tailscale ip -4"
    }
  ]
}
